{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [1, 4, { "SwitchCase":1 }],
        "quotes": [1, "single", { "allowTemplateLiterals": true }],
        "eol-last": 1
    }
}
